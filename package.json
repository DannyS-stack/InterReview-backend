{
  "name": "1-sequelize-typescript-migrations-example",
  "version": "1.0.0",
  "description": "Example how to use migrations in sequelize in typescript",
  "main": "server.js",
  "scripts": {
    "dev": "npx nodemon --exec npx ts-node src/server.ts", 
    "clean": "rm -rf dist",
    "build": "npm run clean && node_modules/typescript/bin/tsc",
    "migrate": "npm run build && node_modules/.bin/sequelize db:migrate --env development",
    "migrate:undo": "npm run build && node_modules/.bin/sequelize db:migrate:undo --env development",
    "migrate:undo:all": "npm run build && node_modules/.bin/sequelize db:migrate:undo:all --env development"
  },
  "keywords": [
    "sequelize",
    "nodejs",
    "migrations",
    "typescript"
  ],
  "author": {
    "url": "https://codeburn.io",
    "name": "Herbert Hart"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DannyS-stack/InterReview-backend.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^7.12.1",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "@types/sequelize": "^4.28.6",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
  }
}
